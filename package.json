{
  "name": "@uupm/cli",
  "version": "0.0.8",
  "description": "uupm - a Unity Package Manager",
  "author": "Zoranner",
  "license": "MIT",
  "main": "./dist/index.js",
  "exports": "./dist/index.js",
  "bin": {
    "uupm": "dist/index.js"
  },
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "scripts": {
    "build": "node es-build.js",
    "build:watch": "nodemon",
    "format": "prettier --write --parser=typescript src/**/*.ts"
  },
  "keywords": [
    "cli",
    "unity",
    "package",
    "manager"
  ],
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^5.3.0",
    "cli-spinners": "^2.9.2",
    "commander": "^12.1.0",
    "figlet": "^1.8.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.7",
    "ora": "^8.1.0",
    "semver": "^7.6.3",
    "tar": "^7.4.3",
    "tslib": "^2.8.0",
    "typescript-collections": "^1.3.3",
    "unzipper": "^0.10.14",
    "update-notifier": "^5.1.0",
    "xml2js": "^0.6.2",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/update-notifier": "^5.1.0",
    "esbuild": "^0.20.2",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  }
}